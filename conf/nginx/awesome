server {
	listen    	80;
	root 	  	/srv/awesome_blog/www;
	access_log	/srv/awesome_blog/log/access_log;
	error_log	/srv/awesome_blog/log/error_log;

	
	server_name     losser.fyf.com; #配置域名
		
	# 处理静态资源:
	#location ~ ^\/static\/.*${
	#	root /srv/awesome_blog/www/static;
	#}
	#动态请求转发到9000端口:
	location / {
		proxy_pass	  http://127.0.0.1:9000;
		proxy_set_header  X-Real-IP $remote_addr;
		proxy_set_header  Host $host;
		proxy_set_header  X-Forwarded-For $proxy_add_x_forwarded_for;
	}
	

}
